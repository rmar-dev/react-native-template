{
    "parser": "babel-eslint",
    "env": {
        "jest": true,
        "browser": true
    },
    "plugins": [
        "react"
    ],
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": [
            2,
            {
                "extensions": [".js", ".jsx"]
            }

        ]
    }
}
